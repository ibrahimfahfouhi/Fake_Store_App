{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=.;Initial Catalog=FakeStore;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "Attachments": {
    "AllowedExtensions": "jpg;jpeg;bmp;png",
    "MaxSizeInMegaBytes": "jpg;jpeg;bmp;png",
    "EnableCompression": true
  }
}
